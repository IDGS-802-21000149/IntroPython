'''
Las tuplas inmutabes
''' 

tupla =("uno", "dos", "tres")
print(tupla)
print(type(tupla))

tuplasVariosDatos = (12,True,23.6,"Nombre",12+3j)
print(tuplasVariosDatos)

tuplasConTuplas=(1,2,3,4,(1,2,3))
print(tuplasConTuplas[3])
print(tuplasConTuplas[-2])
print(tuplasConTuplas[0:2])

a,b,c = tupla

print(a)
print(b)
print(c)

